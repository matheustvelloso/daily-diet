{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "*",
        "dist/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@theme/*": [
        "src/themes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ]
    }
  }
}